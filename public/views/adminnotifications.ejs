                        </div>
                    </div>
                    <div class="row align-items-center mt-4">
                        <div class="col">
                            <ul class="nav nav-tabs overflow-x">
                                <li class="nav-item">
                                    <a href="/dashboard" class="nav-link">
                                        Dashboard
                                    </a>
                                </li>
    
                                <li class="nav-item">
                                    <a href="/users/admintasks" class="nav-link">
                                        Tasks
                                    </a>
                                </li>
    
                                <li class="nav-item">
                                    <a href="/users/admintips" class="nav-link">
                                        Tips
                                    </a>
                                </li>
                              
                                <li class="nav-item">
                                    <a href="/users/admrrifis" class="nav-link active">
                                       User Display Settings
                                    </a>
                                </li>
                              
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <div class="slice slice-sm bg-section-secondary">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-9">
                    <!-- Title -->
                    <h5 class="mb-3">Payments Centre Page</h5>
                    <!-- Card -->
                    <div class="row align-items-centre">
                    <div class="col-md-5">
                    <div class="card">
                        <div class="list-group list-group-flush">
                            <div class="list-group-item d-flex w-100 justify-content-between">
                                <div>
                                    <div class="custom-control custom-switch">
